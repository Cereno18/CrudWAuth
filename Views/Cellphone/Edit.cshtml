@model CrudWAuth.Entities.Cellphone
<form method="post">
    <div class="form-group">
        <hidden label class="form-label">Id</hidden>
        <input type="text" class="form-control" id="exampleInputEmail1" asp-for="Id">
    </div>
    <div class="form-group">
        <label class="form-label">Brand</label>
        <input type="text" class="form-control" id="exampleInputEmail1" asp-for="Brand" placeholder="Enter Brand">
    </div>
    <div class="form-group">
        <label class="form-label">Value</label>
        <input type="number" class="form-control" asp-for="CpValue" placeholder="Value">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
